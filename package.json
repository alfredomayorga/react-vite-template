{
  "name": "react-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "npm": "please-use-bun",
    "yarn": "please-use-bun",
    "bun": "^0.1.13"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect ' vite",
